{
  "name": "Edge Shortcut",
  "author": "meagar,trdey,nagou,amwate",
  "description": "Give shortcuts to the URLs on the webpage",
  "version": "1.0",
  "icons": {
  },
  "permissions": [
  ], 
  "browser_action": {
    "default_icon": {
        "20":"images/color-changer20.png",
        "40":"images/color-changer40.png"
    },
    "default_title": "Edge Shortcut",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [
        "<all_urls>"
    ],
    "js": [""],
    "run_at": "document_end"
}],
  "background": {
    "scripts": [""],
    "persistent": true
  }
}
